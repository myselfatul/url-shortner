generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UrlMapping {
  id        Int      @id @default(autoincrement())
  longUrl   String   @unique
  shortUrl  String   @unique
  createdAt DateTime @default(now())

  @@map("urlMapping")
}
